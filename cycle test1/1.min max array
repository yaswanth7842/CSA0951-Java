public class minmax {
    public static void main(String[] args) {
        int a[] = {1, 4, 6, 3, 2, 9, 7, 5};
        int min = a[0];
        int max = a[0];
        for (int i = 0; i < a.length; i++) {
            if (a[i] < min) {
                min = a[i];
            }
        }
        System.out.println(min);
        for (int j = 0; j < a.length; j++) {
            if (a[j] > max) {
                max = a[j];
            }
        }
        System.out.println(max);
    }
}
