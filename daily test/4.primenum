public class prime {
        public static void main(String[] args) {
            for (int i = 1; i <= 50; i++) {
                int count=0;
                for (int j =2; j <= i/2; j++) {
                    if(i%j==0) {
                        count = count + i;
                    }
                }
                if(count==0)
                    System.out.println(i);
            }
            System.out.println("\n");
        }
    }
